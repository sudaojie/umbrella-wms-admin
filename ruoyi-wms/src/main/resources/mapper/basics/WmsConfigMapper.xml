<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wms.basics.mapper.WmsConfigMapper">

    <resultMap type="com.ruoyi.wms.basics.domain.WmsConfig" id="WmsConfigResult">
        <result property="id" column="id"/>
        <result property="wmsConfigKey" column="wms_config_key"/>
        <result property="wmsConfigName" column="wms_config_name"/>
        <result property="wmsConfigValue" column="wms_config_value"/>
        <result property="enableStatus" column="enable_status"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <select id="select" resultType="com.ruoyi.wms.basics.domain.WmsConfig">
        select
            id,
            wms_config_key,
            wms_config_name,
            wms_config_value,
            enable_status,
            del_flag,
            create_by,
            create_time,
            update_by,
            update_time,
            remark
        from
            wms_config
        <where>
            ${ew.sqlSegment}
        </where>
        order by create_time desc
    </select>

    <select id="checkData" resultType="com.ruoyi.wms.basics.domain.WmsConfig">
        SELECT
        wms_config_key
        FROM
        wms_config
        WHERE
        del_flag = 0 and wms_config_key = #{ew.wmsConfigKey}
        <if test="ew.id!=null and ew.id!=''">
            and id != #{ew.id}
        </if>
    </select>

</mapper>
