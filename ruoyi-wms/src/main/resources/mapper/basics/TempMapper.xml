<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wms.basics.mapper.TempMapper">

    <resultMap type="com.ruoyi.wms.basics.domain.Temp" id="WmsWarehouseTempResult">
        <result property="id" column="id"/>
        <result property="tempId" column="temp_id"/>
        <result property="tempName" column="temp_name"/>
        <result property="module" column="module"/>
        <result property="fileName" column="file_name"/>
        <result property="fileKey" column="file_key"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <select id="select" resultType="com.ruoyi.wms.basics.domain.Temp">
        select
        id,
        temp_id,
        temp_name,
        module,
        file_name,
        file_key,
        del_flag,
        create_by,
        create_time,
        update_by,
        update_time,
        remark
        from
        wms_warehouse_temp
        <where>
            ${ew.sqlSegment}
        </where>
        order by create_time desc
    </select>
    <select id="checkTempId" resultType="java.lang.Integer">
        select count(*) from wms_warehouse_temp where temp_id=${object.tempId} and del_flag = '0'
        <if test="object.id!=null and object.id!=''">
            and id != #{object.id}
        </if>
    </select>
    <select id="selectTempByTempId" resultType="com.ruoyi.wms.basics.domain.Temp">
        select * from wms_warehouse_temp where temp_id=${tempId} and del_flag = '0'
    </select>

</mapper>
