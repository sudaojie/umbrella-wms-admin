<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wms.nolist.mapper.NolistWaitMapper">

    <resultMap type="com.ruoyi.wms.nolist.domain.NolistWait" id="NolistWaitResult">
        <result property="id" column="id"/>
        <result property="trayCode" column="tray_code"/>
        <result property="listingStatus" column="listing_status"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <select id="select" resultType="com.ruoyi.wms.nolist.domain.NolistWait">
        select
            id,
            tray_code,
            listing_status,
            del_flag,
            create_by,
            create_time,
            update_by,
            update_time,
            remark
        from
            wms_nolist_wait
        <where>
            ${ew.sqlSegment}
        </where>
    </select>

</mapper>
